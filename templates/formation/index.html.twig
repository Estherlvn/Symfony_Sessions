{% extends 'base.html.twig' %}

{% block title %}Nos formations{% endblock %}

{% block body %}


<h1>Nos formations</h1>

<ul>
    {% for formation in formations %}
        <li>
            <a href="{{ path('app_formation_show', {'id': formation.id}) }}">
                {{ formation.nomFormation }}
            </a>
        </li>
    {% endfor %}
</ul>


{% endblock %}
