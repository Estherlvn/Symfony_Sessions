{% extends 'base.html.twig' %}

{% block title %}Liste de tous les modules{% endblock %}

{% block body %}

    <h1>Tous les modules</h1>

    <ul>
        {% for module in modules %}
            <li>

                {{ module.nomModule }}

            </li>
        {% endfor %}
    </ul>


    <a href="{{ path('new_module') }}">Ajouter un module</a>


{% endblock %}
